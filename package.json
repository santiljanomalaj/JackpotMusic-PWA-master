{
  "name": "jackpot-music-games-pwa",
  "author": "Jackpot Music Games",
  "description": "Jackpot Music Games - Progressive Web App",
  "homepage": "https://app.jackpot.game",
  "private": true,
  "scripts": {
    "start": "npm run serve:dev",
    "serve:dev": "ionic serve --devapp",
    "build:clean": "ionic clean",
    "build:dev": "JMG_ENV=development ionic build",
    "build:staging": "set JMG_ENV=staging && ionic build --configuration=staging",
    "build:production": "JMG_ENV=production ionic build --prod",
    "lint:ts": "tslint \"src/**/*.ts\" --type-check --project tsconfig.json || true",
    "lint:css": "stylelint \"src/**/*.css\" || true",
    "lint:scss": "stylelint \"src/**/*.scss\" --syntax=scss || true",
    "lint-staged": "lint-staged",
    "heroku-postbuild": "npm run build:${JMG_ENV} --prod",
    "deploy": "npm run build:production && open www"
  },
  "dependencies": {
    "@angular/animations": "^8.2.8",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/pwa": "^0.803.6",
    "@angular/router": "~8.1.2",
    "@angular/service-worker": "^8.2.8",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/geolocation": "^5.13.0",
    "@ionic-native/insomnia": "^5.24.0",
    "@ionic-native/local-notifications": "^5.14.0",
    "@ionic-native/push": "^5.13.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic-native/vibration": "^5.14.0",
    "@ionic/angular": "^4.7.1",
    "@ionic/storage": "^2.2.0",
    "@sentry/browser": "^5.15.5",
    "@types/lodash": "^4.14.150",
    "angular2-moment": "^1.9.0",
    "cordova-android": "^8.0.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-badge": "^0.8.8",
    "cordova-plugin-geolocation": "^4.0.2",
    "cordova-plugin-local-notification": "^0.9.0-beta.2",
    "cordova-plugin-vibration": "^3.1.1",
    "cordova-support-google-services": "^1.1.0",
    "core-js": "^2.5.4",
    "howler": "^2.1.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "phonegap-plugin-multidex": "^1.0.0",
    "phonegap-plugin-push": "^2.2.3",
    "pwa": "^1.9.7",
    "rxjs": "~6.5.1",
    "stylelint": "^11.0.0",
    "sw-toolbox": "^3.6.0",
    "tslib": "^1.9.0",
    "un": "0.0.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.801.2",
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/core": "~8.1.2",
    "@angular-devkit/schematics": "~8.1.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@ionic/angular-toolkit": "~2.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.7",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.1.2",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^4.1.1",
    "cordova-plugin-splashscreen": "^5.0.3",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "ionic": "^5.4.16",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^9.4.0",
    "markdownlint": "^0.16.0",
    "pre-commit": "^1.2.2",
    "protractor": "~5.4.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "cordova": {
    "plugins": {
      "phonegap-plugin-push": {
        "SENDER_ID": "853687662364",
        "ANDROID_SUPPORT_V13_VERSION": "27.+",
        "FCM_VERSION": "11.6.2"
      },
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-geolocation": {
        "GEOLOCATION_USAGE_DESCRIPTION": "This is required to find games near you"
      },
      "cordova-plugin-vibration": {},
      "cordova-plugin-local-notification": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  },
  "lint-staged": {
    "*.ts": "tslint",
    "*.css": "stylelint",
    "*.scss": "stylelint --syntax=scss"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
